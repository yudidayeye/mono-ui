{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome 5173",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "pnpm --filter @monouixc/styles run build:theme",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "console": "integratedTerminal",
      "runtimeArgs": ["--filter", "@monouixc/styles", "run-script", "build:theme"],
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "pnpm --filter @monouixc/icons run build",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "console": "integratedTerminal",
      "runtimeArgs": ["--filter", "@monouixc/icons", "run-script", "build"],
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "pnpm --filter @monouixc/button run build",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "console": "integratedTerminal",
      "runtimeArgs": ["--filter", "@monouixc/button", "run-script", "build"],
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "pnpm --filter @monouixc/docs run dev",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["--filter", "@monouixc/docs", "run-script", "dev"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "name": "调试 typedoc.ts",
      "request": "launch",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["--filter", "@monouixc/docs", "exec", "tsx", "scripts/typedoc.ts"],
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    }
  ]
}
