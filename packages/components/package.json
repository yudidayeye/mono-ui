{
  "name": "@monouijin/components",
  "version": "1.0.0",
  "description": "",
  "keywords": [
    "vue",
    "ui",
    "component library"
  ],
  "author": "yudidayeye",
  "license": "ISC",
  "homepage": "https://github.com/yudidayeye/mono-ui/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yudidayeye/mono-ui.git"
  },
  "bugs": {
    "url": "https://github.com/yudidayeye/mono-ui/issues"
  },
  "scripts": {
    "build:package": "vite build --mode package",
    "build:full": "vite build --mode full",
    "build:full-min": "vite build --mode full-min",
    "build": "pnpm run build:package && pnpm run build:full && pnpm run build:full-min"
  },
  "main": "./dist/monouijin-components.umd.js",
  "module": "./dist/monouijin-components.mjs",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/monouijin-components.umd.js",
      "import": "./dist/monouijin-components.mjs",
      "types": "./dist/src/index.d.ts"
    },
    "./*": "./*",
    "./style/*": "./dist/style/*"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "peerDependencies": {
    "vite": ">=3.0.0",
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "@monouijin/button": "workspace:^",
    "@monouijin/config-provider": "workspace:^",
    "@monouijin/input": "workspace:^",
    "@monouijin/styles": "workspace:^",
    "@monouijin/utils": "workspace:^"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
